  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha",
    "test:live": "mocha --watch",
    "test:benchmark": "babel-node test/benchmarks/benchmarks.js",
    "watch": "gaze 'npm run build' 'src/**/*.js'",
    "lint": "eslint src test",
    "flow": "flow check",
