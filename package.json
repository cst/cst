{
  "name": "cst",
  "version": "0.0.15",
  "description": "JavaScript CST Implementation",
  "author": "Marat Dulin",
  "contributors": [
    {
      "name": "Henry Zhu",
      "email": "hi@henryzoo.com"
    }
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha test/utils.js test/lib test/integrated src --recursive --compilers js:babel-core/register",
    "test:live": "mocha test/utils.js test/lib test/integrated src --recursive --compilers js:babel-core/register --watch",
    "test:benchmark": "babel-node test/benchmarks/benchmarks.js",
    "lint": "npm run lint:jscs",
    "lint:jscs": "jscs src test",
    "build": "babel src -d lib",
    "precommit": "npm test",
    "release": "npm run build && npm publish",
    "postpublish": "rm -Rf lib"
  },
  "devDependencies": {
    "babel-cli": "^6.1.1",
    "babel-core": "^6.0.2",
    "babel-plugin-transform-class-properties": "^6.0.2",
    "babel-plugin-transform-flow-strip-types": "^6.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.0.2",
    "babel-preset-es2015": "^6.0.8",
    "benchmark": "^1.0.0",
    "chai": "^3.0.0",
    "chalk": "^1.0.0",
    "esprima-fb": "15001.1001.0-dev-harmony-fb",
    "husky": "^0.8.1",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.1.0",
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "babel-types": "^6.3.14",
    "babylon": "^6.0.2"
  }
}
