    "precommit": "npm test",
    "release": "npm run build && npm publish",
    "postpublish": "rm -Rf lib"
  },
  "devDependencies": {
    "babel-cli": "7.0.0-alpha.18",
    "babel-helper-fixtures": "7.0.0-alpha.18",
    "babel-eslint": "8.0.0-alpha.17",
    "babel-plugin-transform-class-properties": "7.0.0-alpha.18",
    "babel-plugin-transform-flow-strip-types": "7.0.0-alpha.18",
    "babel-plugin-transform-object-rest-spread": "7.0.0-alpha.18",
    "babel-preset-es2015": "7.0.0-alpha.18",
    "babel-register": "7.0.0-alpha.18",
    "benchmark": "^2.1.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.1",
    "eslint": "~3.8.1",
    "eslint-config-google": "~0.7.0",
    "eslint-plugin-flowtype": "~2.25.0",
    "gaze-cli": "^0.2.0",
    "husky": "^0.11.4",
    "lodash.foreach": "^4.5.0",
    "lodash.get": "^4.4.2",
    "mocha": "^3.1.2"
  },
  "dependencies": {
    "babylon": "7.0.0-beta.19",
    "source-map-support": "^0.4.0"
  },
  "files": [
    "lib"
  ]
}
